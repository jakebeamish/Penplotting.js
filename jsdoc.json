{
  "source": {
    "include": ["source"],
    "exclude": ["source/tests"]
  },
  "opts": {
    "encoding": "utf8",
    "readme": "./README.md",
    "destination": "./docs",
    "recurse": true,
    "verbose": true
  },
  "plugins": ["plugins/markdown"],
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  }
}
